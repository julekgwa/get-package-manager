{
    "env": {
        "node": true,
        "jest": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "padded-blocks": ["error", { "switches": "never" }],
        "newline-after-var":"error"
    }
}
